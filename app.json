{
  "expo": {
    "name": "news-api-app",
    "slug": "news-api-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/apple-icon-120x120.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "news-api-app"
    },
    "web": {
      "favicon": "./assets/News-favicon.png"
    }
  }
}
